# Environment
ENVIRONMENT=development

# Database configuration
DATABASE_URL="supabase_database_url"
DATABASE_NAME=postgres
SQL_ECHO=False

# API configuration
API_SECRET_KEY=your_secret_key_here
API_ALGORITHM=HS256
API_ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS configuration
CORS_ORIGINS=["http://localhost:3000","http://localhost:8000","http://127.0.0.1:3000","http://127.0.0.1:8000"]

# OpenAI
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4o-mini

# SERP API
SERPAPI_KEY=your-serpapi-key

# Twitter/Nitter
TWITTER_NITTER_BASE_URL=https://nitter.net
TWITTER_API_KEY=your-twitter-api-key
TWITTER_API_SECRET=your-twitter-api-secret
TWITTER_ACCESS_TOKEN=your-twitter-access-token
TWITTER_ACCESS_SECRET=your-twitter-access-secret

# Nitter instances as JSON array string
NITTER_INSTANCES=["https://nitter.net","https://nitter.privacydev.net","https://nitter.poast.org","https://nitter.woodland.cafe"]

# Redis
REDIS_URL=redis://localhost:6379/0

# Celery
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# Logging
LOG_LEVEL=INFO

# Development Specific
DEV_API_KEY=your-dev-api-key